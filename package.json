{
  "name": "john-morningshop-youtube",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:nowatch": "react-scripts test --verbose --runInBand --watchAll=false",
    "test:coverage": "react-scripts test --coverage --verbose --runInBand --watchAll=false",
    "lint:prettier": "prettier src/**/*.{ts,tsx} --write",
    "eject": "react-scripts eject"
  },
  "importSort": {
    ".ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^7.26.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.2.2",
    "import-sort-style-module": "^6.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "@reduxjs/toolkit": "^1.4.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.11",
    "@types/styled-components": "^5.1.4",
    "@types/webpack-env": "^1.15.3",
    "axios": "^0.21.0",
    "immer": "^8.0.0",
    "prettier": "^2.1.2",
    "prettier-plugin-import-sort": "^0.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "7.2.2",
    "react-scripts": "4.0.0",
    "redux": "4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.6.3",
    "styled-components": "^5.2.1",
    "typesafe-actions": "^4.0.0",
    "typescript": "4.0.2"
  }
